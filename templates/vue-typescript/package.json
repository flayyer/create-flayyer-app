{
  "name": "{{ replace-slug }}",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "flyyer start",
    "build": "flyyer build",
    "deploy": "flyyer deploy",
    "format": "xo --fix"
  },
  "dependencies": {
    "@flyyer/types": "^2.0.2",
    "@flyyer/proxy": "^2.0.0",
    "@flyyer/variables": "^2.0.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@flyyer/cli": "{{ replace-cli-version }}",
    "@vue/component-compiler-utils": "^3.2.0",
    "dotenv": "^10.0.0",
    "eslint-config-xo-typescript": "^0.42.0",
    "eslint-config-xo-vue": "^3.1.23",
    "eslint-plugin-unicorn": "^33.0.1",
    "eslint-plugin-vue": "^7.11.1",
    "typescript": "^4.2.4",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12",
    "xo": "^0.39.1"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "extends": "xo-vue",
    "extensions": [
      "ts",
      "js",
      "vue"
    ],
    "rules": {
      "new-cap": "off",
      "no-unused-vars": "off",
      "import/no-unresolved": "off",
      "unicorn/prefer-module": "off"
    }
  }
}
