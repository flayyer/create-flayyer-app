{
  "name": "{{ replace-slug }}",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "flayyer start",
    "build": "flayyer build",
    "deploy": "flayyer deploy",
    "format": "xo --fix"
  },
  "dependencies": {
    "@flayyer/flayyer-types": "^1.11.1",
    "@flayyer/variables": "^1.5.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@flayyer/cli": "{{ replace-cli-version }}",
    "@vue/component-compiler-utils": "^3.2.0",
    "dotenv": "^10.0.0",
    "eslint-config-xo-typescript": "^0.42.0",
    "eslint-config-xo-vue": "^3.1.23",
    "eslint-plugin-unicorn": "^33.0.1",
    "eslint-plugin-vue": "^7.11.1",
    "typescript": "^4.2.4",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12",
    "xo": "^0.40.3"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "extends": "xo-vue",
    "extensions": [
      "ts",
      "js",
      "vue"
    ],
    "rules": {
      "new-cap": "off",
      "no-unused-vars": "off",
      "import/no-unresolved": "off",
      "unicorn/prefer-module": "off"
    }
  }
}
