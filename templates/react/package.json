{
  "name": "{{ replace-slug }}",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "flayyer start",
    "build": "flayyer build",
    "deploy": "flayyer deploy",
    "format": "xo --fix"
  },
  "dependencies": {
    "@flayyer/flayyer-types": "^1.11.1",
    "@flayyer/variables": "^1.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@flayyer/cli": "{{ replace-cli-version }}",
    "dotenv": "^8.2.0",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "xo": "^0.39.1"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "extends": "xo-react",
    "rules": {
      "new-cap": "off",
      "no-unused-vars": "off",
      "react/prop-types": "off",
      "react/function-component-definition": "off"
    }
  }
